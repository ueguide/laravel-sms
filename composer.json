{
    "name": "thelhc/laravel-sms",
    "description": "Laravel SMS manager",
    "authors": [
        {
            "name": "Aaron Michael Music",
            "email": "aaronmichaelmusic@gmail.com"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/auth": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
        "illuminate/contracts": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
        "illuminate/database": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
        "aws/aws-sdk-php": "^3.52"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4 || ~6.0",
        "orchestra/testbench": "~3.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "TheLHC\\SMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheLHC\\SMS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
